		System.out.println("press 3 for priority shipping");
		int shipping = keyboard.nextInt();
		if (location == 1)
		{
		    if (shipping == 1)
		    {
		        System.out.println("shipping starts at $5.99");
		    }
		    else if (shipping == 2)
		    {
		        System.out.println("shipping starts at $10.99");
		    }
		    else if (shipping == 3)
		    {
		        System.out.println("shipping starts at $13.99");
		    }
		}
		else if (location == 2)
		{
		    if (shipping == 1)
		    {
		        System.out.println("shipping starts at $4.99");
		    }
		    else if (shipping == 2)
		    {
		        System.out.println("n/a");
		    }
		    else if (shipping == 3)
		    {
		        System.out.println("shipping starts at $14.99");
		    }
		}
		else if (location == 3)
		{
		    if (shipping == 1)
		    {
		        System.out.println("shipping starts at $6.99");
		    }
		    else if (shipping == 2)
		    {
		        System.out.println("shipping starts at $13.99");
